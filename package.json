{
	"name": "remix-vite-template",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"start": "remix-serve ./build/index.js",
		"db": "drizzle-kit generate:sqlite --config=drizzle.config.ts && drizzle-kit push:sqlite --config=drizzle.config.ts",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:migrate": "tsx ./app/db/migrate",
		"db:studio-remote": "drizzle-kit studio --config=drizzle.config.ts",
		"db:studio-local": "drizzle-kit studio --config=drizzle_local.config.ts",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@libsql/client": "0.4.0-pre.7",
		"@radix-ui/react-icons": "^1.3.0",
		"@remix-run/node": "^2.4.1",
		"@remix-run/react": "^2.4.1",
		"@remix-run/serve": "^2.4.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"drizzle-orm": "^0.29.3",
		"isbot": "^4.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.12.0",
		"tailwind-merge": "^2.2.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@remix-run/dev": "^2.4.1",
		"@remix-run/eslint-config": "^2.4.1",
		"@types/morgan": "^1.9.9",
		"@types/react": "^18.2.47",
		"@types/react-dom": "^18.2.18",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unicorn": "^50.0.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"morgan": "^1.10.0",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"remix-development-tools": "^3.7.1",
		"remix-flat-routes": "^0.6.4",
		"tailwindcss": "^3.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-turbo-console": "^0.3.2",
		"vite-tsconfig-paths": "^4.2.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
